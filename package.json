{
  "name": "nwjs-with-cra",
  "main": "./public/main.js",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "node-remote": "http://localhost:3000",
  "build": {
    "nwVersion": "0.42.5"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "postinstall": "cross-env-shell npm install --no-save nw@$npm_package_build_nwVersion --nwjs_build_type=sdk",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "nwjs-reactbuild": "nw .",
    "prenwjs-reactbuild": "yarn build",
    "nwjs-pack": "build --tasks win-x86,win-x64,linux-x86,linux-x64,mac-x64 --mirror https://dl.nwjs.io/ .",
    "prenwjs-pack": "yarn build",
    "nwjs-dev": "cross-env BROWSER=none NWJS_START_URL=http://localhost:3000 concurrently \"yarn start\" \"wait-on http://localhost:3000 && nw .\" "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "cross-env": "^6.0.3",
    "nwjs-builder-phoenix": "^1.15.0",
    "wait-on": "^3.3.0"
  }
}
